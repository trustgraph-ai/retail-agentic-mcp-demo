@prefix product: <http://example.com/products/> .
@prefix ex: <http://example.com/ns/> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# --- Tents ---
product:TrailSeekerPro
    a schema:Product ;
    rdfs:label "TrailSeeker Pro" ;
    schema:name "TrailSeeker Pro" ;
    schema:description "A durable, 4-season tent for serious adventurers." ;
    schema:sku "TENT-TSP-4P" ;
    schema:brand "AdventureRidge" ;
    schema:category "Tents" ;
    schema:capacity "4"^^xsd:integer ;
    schema:weight "3.5 kg" ;
    schema:price "349.99"^^xsd:decimal ;
    ex:suggestedProduct product:NordicWave, product:SummitPack60 .

product:HikerLite
    a schema:Product ;
    rdfs:label "HikerLite" ;
    schema:name "HikerLite" ;
    schema:description "An ultralight 2-person tent for backpacking." ;
    schema:sku "TENT-HL-2P" ;
    schema:brand "FeatherPeak" ;
    schema:category "Tents" ;
    schema:capacity "2"^^xsd:integer ;
    schema:weight "1.2 kg" ;
    schema:price "279.99"^^xsd:decimal .

